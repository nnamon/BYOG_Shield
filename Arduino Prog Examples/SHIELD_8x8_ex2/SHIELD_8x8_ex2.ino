#include "TimerOne-master\TimerOne.h"
#include "S8x8_Lib.h"
#include "Animate.h"

int num = '0';
//===============================================================================================================================
void setup() {
  S8x8Setup();
  Serial.begin(9600);

  DisplayChar(num);
}
//===============================================================================================================================
void loop() {
  for(int c=0;c<10;c++) {    
    DisplayChar(num + c);
    delay(500);
  }   
}
